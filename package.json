{
  "name": "city-flow",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test:unit:only": "vitest run --config vitest.config.unit.ts",
    "test:react:only": "vitest run --config vitest.config.react.ts",
    "test:unit": "npm run test:unit:only && npm run test:react:only",
    "test:unit:watch": "vitest --config vitest.config.unit.ts",
    "test:react:watch": "vitest --config vitest.config.react.ts",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "npm run test:unit:only -- --coverage && npm run test:react:only -- --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:coverage": "nyc report --reporter=html --reporter=text --reporter=json-summary --reporter=lcov",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "validate": "npm run typecheck && npm run lint && npm run format -- --check",
    "prepare": "husky",
    "dev:e2e": "astro dev --mode test"
  },
